					echo "id_ente: \"required\",";
				} ?>
				oggetto: "required",
				data_attivazioneVis: "required",
				dati_aggiudicatrice: {
					/* required: true, */
					cfCheck: true
				},
				valore_importo_aggiudicazione: "floatCheck",
				importo_liquidato: "floatCheck"
			},
			messages: {
				<? // scelta ente
