				data_attivazioneVis: "required",
				dati_aggiudicatrice: {
					/* required: true, */
					cfCheck: true
				},
				valore_base_asta: "floatCheck",
				valore_importo_aggiudicazione: "floatCheck"
				//importo_liquidato: "floatCheck"
			},
			messages: {
