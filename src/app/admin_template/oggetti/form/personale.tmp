	function toUpper(mystring, fieldId) {
		var sp = mystring.split(' ');
		var wl=0;
		var f ,r;
		var word = new Array();
		for (i = 0 ; i < sp.length ; i ++ ) {
			f = sp[i].substring(0,1).toUpperCase();
			r = sp[i].substring(1).toLowerCase();
			word[i] = f+r;
		}
		newstring = word.join(' ');
		document.getElementById(fieldId).value = newstring;
		return true;
	}
